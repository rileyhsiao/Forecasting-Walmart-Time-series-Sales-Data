        "df = p.merge(calendarPrices, sell_prices, on = ['wm_yr_wk','item_id','store_id'], how = 'inner')\n",
        "df['value'] = p.to_numeric(df['value'], errors='coerce')\n",
        "\n",
        "for column in df.columns:\n",
        "    if df[column].dtype == 'int32':\n",
        "        df[column] = p.to_numeric(df[column], downcast='integer')\n",
        "    if df[column].dtype == 'int16':\n",
        "        df[column] = p.to_numeric(df[column], downcast='integer')\n",
        "    if df[column].dtype == 'float64':\n",
        "        df[column] = df[column].astype('float32')\n",
        "\n",
        "del(calendarPrices)\n",
        "del(sell_prices)\n",
        "df.dtypes"
      ]
