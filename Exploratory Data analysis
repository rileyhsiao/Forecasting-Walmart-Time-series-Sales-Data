      "source": [
        "\n",
        "\n",
        "# Group by 'event_type_1' and calculate the average value for each event type\n",
        "average_by_event_type = df.groupby('event_type_1')['value'].mean()\n",
        "\n",
        "# Calculate the overall average value\n",
        "overall_average = df['value'].mean()\n",
        "\n",
        "# Plot the line graph\n",
        "fig, ax = plt.subplots(figsize=(10, 6))\n",
        "\n",
        "# Line for average sales for each event type\n",
        "average_by_event_type.plot(kind='line', marker='o', color='blue', ax=ax, label='Average Sales by Event Type')\n",
        "\n",
        "# Line for overall average of all sales\n",
        "ax.axhline(overall_average, color='red', linestyle='--', label='Overall Average of All Sales')\n",
        "\n",
        "# Set plot labels and title\n",
        "ax.set_xlabel('Event Type')\n",
        "ax.set_ylabel('Average Sales')\n",
        "ax.set_title('Average Sales for Each Event Type')\n",
        "\n",
        "# Rotate x-axis labels for better readability\n",
        "plt.xticks(rotation=45, ha='right')\n",
        "\n",
        "# Show a legend\n",
        "ax.legend()\n",
        "\n",
        "# Display the plot\n",
        "plt.show()"
      ]
