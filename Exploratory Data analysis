        "\n",
        "# Group by 'event_name_1' and 'cat_id' and calculate the average value for each event name and category\n",
        "average_by_cat_and_month = df.groupby(['event_name_1', 'cat_id'])['value'].mean().unstack()\n",
        "\n",
        "# Plot a line for each cat_id\n",
        "fig, ax = plt.subplots(figsize=(12, 6))  # Increase the figure size\n",
        "\n",
