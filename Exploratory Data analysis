        "\n",
        "# Use seaborn color palette for distinct colors\n",
        "colors = sns.color_palette(\"Set1\", n_colors=len(average_by_event['event_type_1'].unique()))\n",
        "\n",
        "for i, event_type in enumerate(average_by_event['event_type_1'].unique()):\n",
        "    event_type_data = average_by_event[average_by_event['event_type_1'] == event_type]\n",
        "    ax.bar(event_type_data['event_name_1'], event_type_data['value'], label=event_type, color=colors[i])\n",
