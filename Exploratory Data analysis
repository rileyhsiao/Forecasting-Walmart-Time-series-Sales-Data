      "source": [
        "columns_to_encode = ['item_id', 'dept_id', 'cat_id', 'store_id', 'd', 'weekday',\n",
        "                     'event_name_1', 'event_type_1', 'event_name_2', 'event_type_2']\n",
        "\n",
        "# Columns to drop\n",
        "columns_to_drop = ['id', 'state_id', 'date', 'wm_yr_wk']\n",
        "\n",
        "# Initialize LabelEncoder\n",
        "label_encoder = LabelEncoder()\n",
        "\n",
        "# Perform integer encoding for non-numeric columns\n",
        "for column in columns_to_encode:\n",
        "    if df[column].dtype == 'object':\n",
        "        # Convert non-numeric values to strings before encoding\n",
        "        df[column] = label_encoder.fit_transform(df[column].astype(str))\n",
        "\n",
        "# Drop specified columns\n",
        "df.drop(columns=columns_to_drop, inplace=True)\n",
        "\n",
        "# Display the updated DataFrame\n",
        "print(df.head())"
      ]
